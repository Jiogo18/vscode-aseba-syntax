{
  "name": "aseba-syntax",
  "displayName": "Aseba Syntax",
  "description": "Aseba scripts syntax highlighting",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jiogo18/vscode-aseba-syntax"
  },
  "license": "MIT",
  "publisher": "Jiogo18",
  "contributes": {
    "languages": [{
      "id": "aseba",
      "aliases": ["Aseba", "aseba"],
      "extensions": [".aesl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "aseba",
      "scopeName": "source.aseba",
      "path": "./syntaxes/aseba.tmLanguage.json"
    }]
  }
}
